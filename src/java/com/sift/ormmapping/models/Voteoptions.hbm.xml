<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sift.easycoopfin.models.Voteoptions" table="voteoptions" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="voteId" column="vote_id" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="voteOption" column="vote_option" type="string" length="100" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
