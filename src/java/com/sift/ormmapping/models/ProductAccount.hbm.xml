<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sift.easycoopfin.models.ProductAccount" table="ProductAccount" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="productId" column="product_id" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="productAccountTypeCode" column="product_account_type_code" type="string" length="10" not-null="true" lazy="false"/>
		<property name="glAccountNumber" column="gl_account_number" type="string" length="50" not-null="true" lazy="false"/>
                <property name="branchId" column="branch_id" type="integer" length="10" not-null="false" lazy="false"/>
                <property name="companyId" column="company_id" type="integer" length="10" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
