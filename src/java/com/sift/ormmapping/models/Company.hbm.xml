<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sift.easycoopfin.models.Company" table="Company" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
                
		<!-- <property name="companyId" column="company_id" type="integer" length="10" not-null="true" lazy="false"/>-->
                <property name="code" column="code" type="string" length="100" not-null="true" lazy="false"/>
                <property name="name" column="name" type="string" length="255" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
